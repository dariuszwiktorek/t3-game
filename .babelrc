{
  "plugins": [
    ["babel-plugin-macros"],
    [
        "babel-plugin-styled-components",
        {
            "minify": true,
            "transpileTemplateLiterals": false,
            "pure": true,
            "displayName": true, // generate another classname
            "fileName": true, // generate another classname
            "preprocess": false,
            "meaninglessFileNames": ["index", "styles"]
        }
    ]
  ]

}